{{#link-to 'article-detail' article.id}}<div class="well">
  <img src={{article.image}} class="img-responsive">
  <div class="previewText">
    <h2>{{article.title}}</h2>
    <p>{{preview article.body}}</p>
    <h3>{{article.author}}</h3>
    <h3>{{moment-to-now article.timestamp date}}</h3>
  </div>
</div>
{{/link-to}}
